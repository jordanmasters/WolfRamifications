Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"getVowels", "[", "word_", "]"}], " ", ":=", " ", 
   RowBox[{"Replace", "[", "\n", "\t", 
    RowBox[{
     RowBox[{"WordData", "[", 
      RowBox[{"word", ",", " ", "\"\<PhoneticForm\>\""}], "]"}], ",", "\n", 
     "\t", 
     RowBox[{"{", "\n", "\t\t", 
      RowBox[{
       RowBox[{
        RowBox[{"s_", "?", "StringQ"}], " ", "\[RuleDelayed]", " ", 
        RowBox[{"StringReplace", "[", 
         RowBox[{"s", ",", " ", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{"Alternatives", " ", "@@", " ", "consonants"}], ")"}], 
            "|", "\"\<\:02cc\>\"", "|", "\"\<\:02c8\>\""}], " ", "\[Rule]", 
           " ", "\"\<\>\""}]}], "]"}]}], ",", "\n", "\t\t", 
       RowBox[{"any_", " ", "\[RuleDelayed]", " ", 
        RowBox[{"Missing", "[", "\"\<NotAvailable\>\"", "]"}]}]}], "\n", "\t",
       "}"}]}], "\n", "]"}]}], "\[IndentingNewLine]"}]], "Input",
 CellChangeTimes->{{3.707234309257423*^9, 3.707234311556719*^9}, {
   3.707235876852704*^9, 3.707235916889969*^9}, 3.7072360037653217`*^9, 
